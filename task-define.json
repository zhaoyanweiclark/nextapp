{
  "containerDefinitions": [ 
    { 
      "essential": true,
      "image": "602222598878.dkr.ecr.ap-southeast-2.amazonaws.com/nextapi:latest",
      "name": "nextapi",
      "portMappings": [ 
        { 
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "cpu": 1024,
      "memory": 2048,
      "environment": [
        {
          "name": "POSTGRES_HOST",
          "value": ""
        },
        {
          "name": "POSTGRES_USER",
          "value": ""
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": ""
        },
        {
          "name": "POSTGRES_DB",
          "value": ""
        }
      ]
    }
  ],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::602222598878:role/ecsTaskExecutionRole",
  "family": "nextapi",
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [ "FARGATE" ]
}